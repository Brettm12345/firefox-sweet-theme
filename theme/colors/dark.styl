/* Variables that start with --sweet- are added by me and are assigned
 * to elements somewhere in this code. The rest of the variables are
 * built-in in Firefox, so you need to add an i if you wanna
 * override them. */
:root
  /* Browser area before a page starts loading */
  --sweet-browser-before-load-background #212337
  /* Toolbars */
  --sweet-toolbar-background #212337
  --sweet-toolbar-color #c8d3f5
  --sweet-toolbar-border-color #0c0e15
  --sweet-inactive-toolbar-background #212337
  --sweet-inactive-toolbar-border-color #202020
  /* Sidebar */
  --sweet-sidebar-background #1e2030
  --sweet-inactive-sidebar-background #1e2030
  /* Popups */
  --sweet-menu-background #212337
  --sweet-menu-border-color #0c0e15
  --sweet-popover-background #212337
  --sweet-popover-border-color #0c0e15
  --sweet-popover-shadow 0 1px 2px rgba(0, 0, 0, 0.3)
  --sweet-popover-button-hover-background #252a3f
  --sweet-popover-separator-color rgba(0, 0, 0, 0.1)
  /* Header bar */
  --sweet-headerbar-background #191b28
  --sweet-headerbar-border-color #070707
  --sweet-headerbar-box-shadow inset 0 1px rgba(238, 238, 236, 0.07)
  --sweet-inactive-headerbar-background linear-gradient(#212337, #212337)
  --sweet-inactive-headerbar-border-color #202020
  --sweet-inactive-headerbar-box-shadow inset 0 1px rgba(238, 238, 236, 0.07)
  /* Buttons */
  --sweet-button-background linear-gradient(to top, #1b1d2c 2px, #1b1d2c)
  --sweet-button-border-color #0c0e15
  --sweet-button-border-accent-color #070707
  --sweet-button-box-shadow inset 0 1px rgba(255, 255, 255, 0.02), 0 1px 2px rgba(0, 0, 0, 0.07)
  --sweet-button-hover-background linear-gradient(to top, #252a3f, #252a3f 1px)
  --sweet-button-active-background #2f334d
  --sweet-button-active-border-color #121523aa
  --sweet-button-active-border-accent-color #000000
  --sweet-button-active-box-shadow inset 0 1px rgba(255, 255, 255, 0)
  --sweet-button-disabled-background #252a3f
  --sweet-button-disabled-border-color #212337
  --sweet-button-disabled-box-shadow inset 0 1px rgba(255, 255, 255, 0)
  --sweet-inactive-button-background linear-gradient(#212337, #212337)
  --sweet-inactive-button-border-color #212337
  --sweet-inactive-button-box-shadow inset 0 1px rgba(255, 255, 255, 0)
  --sweet-button-suggested-action-background linear-gradient(to top, #654ea3 2px, #eaafc8)
  --sweet-button-suggested-action-border-color #654ea3
  --sweet-button-suggested-action-border-accent-color #092444
  --sweet-button-suggested-action-box-shadow inset 0 1px rgba(255, 255, 255, 0.02), 0 1px 2px rgba(0, 0, 0, 0.07)
  --sweet-button-suggested-action-hover-background linear-gradient(to top, #654ea3ec, #eaafc8ec 1px)
  --sweet-button-suggested-action-active-background #654ea3
  --sweet-button-suggested-action-active-border-color #654ea3
  --sweet-button-suggested-action-active-box-shadow inset 0 1px rgba(255, 255, 255, 0)
  --sweet-button-destructive-action-background linear-gradient(to top, #ff416c 2px, #ff4b2b)
  --sweet-button-destructive-action-border-color #ff757f
  --sweet-button-destructive-action-border-accent-color #570b0e
  --sweet-button-destructive-action-box-shadow inset 0 1px rgba(255, 255, 255, 0.02), 0 1px 2px rgba(0, 0, 0, 0.07)
  --sweet-button-destructive-action-hover-background linear-gradient(to top, #ff416cec, #ff4b2bec 1px)
  --sweet-button-destructive-action-active-background #ff757f
  --sweet-button-destructive-action-active-border-color #ff757f
  --sweet-button-destructive-action-active-box-shadow inset 0 1px rgba(255, 255, 255, 0)
  /* URL bar */
  --sweet-urlbar-background #212337
  --sweet-urlbar-border-color #0c0e15
  --sweet-urlbar-box-shadow inset 0 0 0 1px rgba(21, 83, 158, 0)
  --sweet-urlbar-color #c8d3f5
  --sweet-inactive-urlbar-background linear-gradient(#252a3f, #252a3f)
  --sweet-inactive-urlbar-border-color #191a2a
  --sweet-inactive-urlbar-box-shadow none
  --sweet-inactive-urlbar-color #a9b8e8
  --sweet-focused-urlbar-border-color #82aaff
  --sweet-private-urlbar-background #25003e
  /* Tabs */
  --sweet-tabbar-tab-color #828bb8
  --sweet-tabbar-tab-hover-background #828bb820
  --sweet-tabbar-tab-hover-border-bottom-color #0c0e15
  --sweet-tabbar-tab-hover-color #828bb8
  --sweet-tabbar-tab-active-background #222436
  --sweet-tabbar-tab-active-border-bottom linear-gradient(to right, #4976eb, #82aaff, #50c4fa, #86e1fc, #77e0c6, #4fd6be) left bottom #222436 no-repeat
  --sweet-tabbar-tab-active-color #82aaff
  --sweet-tabbar-tab-active-hover-background #212337
  --sweet-inactive-tabbar-tab-color #828bb8
  --sweet-inactive-tabbar-tab-active-background #212337
  --sweet-inactive-tabbar-tab-active-border-bottom-color #82aaff
  --sweet-inactive-tabbar-tab-active-color #828bb8
